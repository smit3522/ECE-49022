function y  = str_com2_array(my_string)
y = [0 0 0 0 0 0];
char_array = convertStringsToChars(my_string);
j=1;
i=1;
count = 0;
markNeg = 0;
while i <= length(char_array)
    if(char_array(i) == '-')
        markNeg = 1;
    end
    
    if(char_array(i)== ',')
        if(markNeg == 1)
            if(count == 2)
                y(j)=(-1*str2double(char_array(i-1)));
            elseif(count == 3)
                y(j)=-1*((str2double(char_array(i-2))*10) + (str2double(char_array(i-1))));
            else
                y(j)=-1*((str2double(char_array(i-3))*100) + (str2double(char_array(i-2))*10) + (str2double(char_array(i-1))));
            end
        else
            if(count == 1)
                y(j)=(str2double(char_array(i-1)));
            elseif(count == 2)
                y(j)=(str2double(char_array(i-2))*10) + (str2double(char_array(i-1)));
            else
                y(j)=(str2double(char_array(i-3))*100) + (str2double(char_array(i-2))*10) + (str2double(char_array(i-1)));
            end
        end
        j=j+1;
        count = 0;
        markNeg = 0;
    else
        count = count + 1;
        if(i == length(char_array))
            if(markNeg == 1)
                if(count == 2)
                    y(j)=-1*(str2double(char_array(i)));
                elseif(count == 3)
                    y(j)=-1*((str2double(char_array(i-1))*10) + (str2double(char_array(i))));
                else
                    y(j)=-1*((str2double(char_array(i-2))*100) + (str2double(char_array(i-1))*10) + (str2double(char_array(i))));
                end
            else
                if(count == 1)
                    y(j)=(str2double(char_array(i)));
                elseif(count == 2)
                    y(j)=(str2double(char_array(i-1))*10) + (str2double(char_array(i)));
                else
                    y(j)=(str2double(char_array(i-2))*100) + (str2double(char_array(i-1))*10) + (str2double(char_array(i)));
                end
            end
        end
    end
    i = i+1;
end
end
